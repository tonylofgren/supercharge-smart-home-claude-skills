# ESPHome Secrets Template
# Copy this to secrets.yaml and fill in your values

# WiFi credentials
wifi_ssid: "YOUR_WIFI_SSID"
wifi_password: "YOUR_WIFI_PASSWORD"

# API encryption key (generate with: esphome dashboard)
# Must be a 32-byte base64 encoded string
api_encryption_key: "GENERATE_WITH_ESPHOME_DASHBOARD"

# OTA password
ota_password: "your_ota_password_here"

# Fallback AP password
fallback_password: "fallback123"
